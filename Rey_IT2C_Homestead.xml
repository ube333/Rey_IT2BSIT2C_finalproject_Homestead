<!-- dicription and how Homestead works -->
<!-- nav bar section starts -->
<nav_bar>
	<!-- if user click home btn the system will redirect to home page -->
	<home type="button">redirecting to home</home>

	<!--if user click game  btn the system will redirect to game page -->
	<game type="button">redirecting to game tictac toe</game>

	<!-- if user click gallery btn the system will redirecting to gallery page -->
	<gallery type="button">redirecting to gallery</gallery>

	<!-- if user click book btn the system will redirecting to book pages -->
	<book type="button">redicting to books</book>

	<!-- if the user click menu btn the menu will colapse and show the admin page -->
	<menu type="button" class="colapse">

		<!-- after menu btn colapse -->
		<admin_gallery_page type="button">

			<!-- input user name and password which is username: admin pass:admin -->
			<input class="username, password">

				<!-- after the user filled the input user shall click the login btn to proceed -->
				<login type="button">Login</login>

				<!-- after login the user can upload image that will display in the gallery page after uploading -->
				<upload_image type="images">upload</upload_image>

				<!-- user can delete the user that is uploaded in the gallery using the admin page -->
				<delete_image type="images">delete</delete_image>
			</input>
		</admin_gallery_page>
	</menu>
</nav_bar>
<!-- nav bay section ends-->

